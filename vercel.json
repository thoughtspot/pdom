{
    "headers": [
        {
            "source": "/(.*)",
            "missing": [
                {
                    "type": "query",
                    "key": "disable-origin-agent-cluster",
                    "value": "true"
                }
            ],
            "headers": [
                {
                    "key": "Origin-Agent-Cluster",
                    "value": "?1"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; connect-src * 'self' https://*.sentry.io; script-src * 'unsafe-eval' 'unsafe-inline'; style-src * 'unsafe-inline' data:; img-src * data: blob: 'self'; font-src *; worker-src * data: blob:; frame-ancestors *; form-action 'self'"
                }
            ]
        }
    ]
}
